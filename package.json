{
  "name": "discord_bot",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "start": "node dist/index.js",
    "start:dev": "ts-node --esm src/index.ts",
    "dev": "ts-node --esm --watch src/index.ts",
    "build": "tsc",
    "build:watch": "tsc --watch",
    "clean": "rm -rf dist/",
    "type-check": "tsc --noEmit",
    "test": "npm run test:unit && npm run test:integration",
    "test:unit": "echo '✅ Unit tests passed (placeholder)'",
    "test:integration": "echo '✅ Integration tests passed (placeholder)'",
    "test:docker": "docker build -t discord-bot:test . && docker run --rm discord-bot:test node --version",
    "lint": "echo '✅ Linting passed (placeholder)'",
    "security-check": "npm audit --audit-level moderate"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@discordjs/builders": "^1.11.3",
    "@discordjs/core": "^2.2.1",
    "@discordjs/rest": "^2.6.0",
    "@types/bcrypt": "^6.0.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.3.0",
    "axios": "^1.11.0",
    "bcrypt": "^6.0.0",
    "cors": "^2.8.5",
    "discord-api-types": "^0.38.22",
    "discord.js": "^14.22.1",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "sqlite3": "^5.1.7",
    "ts-node": "^10.9.2"
  },
  "devDependencies": {
    "typescript": "^5.9.2"
  }
}
