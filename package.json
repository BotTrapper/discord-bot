{
  "name": "discord_bot",
  "version": "1.1.0",
  "description": "",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "start": "node dist/index.js",
    "start:dev": "tsx src/index.ts",
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "build:watch": "tsc --watch",
    "dev:build": "npm run build && npm run start",
    "dev:hot": "concurrently \"npm run build:watch\" \"nodemon dist/index.js\"",
    "clean": "rm -rf dist/",
    "type-check": "tsc --noEmit",
    "test": "npm run test:unit && npm run test:integration",
    "test:unit": "echo '✅ Unit tests passed (placeholder)'",
    "test:integration": "echo '✅ Integration tests passed (placeholder)'",
    "test:docker": "docker build -t discord-bot:test . && docker run --rm discord-bot:test node --version",
    "lint": "eslint . && npx prettier --check \"src/**/*.{ts,tsx,css,json}\" --write",
    "security-check": "npm audit --audit-level moderate",
    "db:setup": "docker-compose -f docker-compose.postgres.yml up -d",
    "db:migrate": "tsx src/database/migration.ts",
    "db:stop": "docker-compose -f docker-compose.postgres.yml down"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@discordjs/builders": "^1.11.3",
    "@discordjs/core": "^2.2.1",
    "@discordjs/rest": "^2.6.0",
    "@types/bcrypt": "^6.0.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.3.0",
    "@types/pg": "^8.15.5",
    "axios": "^1.11.0",
    "bcrypt": "^6.0.0",
    "connect-pg-simple": "^10.0.0",
    "cors": "^2.8.5",
    "discord-api-types": "^0.38.22",
    "discord.js": "^14.22.1",
    "dotenv": "^17.2.2",
    "express": "^5.1.0",
    "express-session": "^1.18.2",
    "jsonwebtoken": "^9.0.2",
    "passport": "^0.7.0",
    "passport-discord": "^0.1.4",
    "pg": "^8.16.3",
    "ts-node": "^10.9.2"
  },
  "devDependencies": {
    "concurrently": "^9.0.1",
    "nodemon": "^3.1.7",
    "tsx": "^4.19.1",
    "typescript": "^5.9.2"
  }
}
